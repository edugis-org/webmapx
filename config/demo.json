{
  "map":
  {
    "label": "Demo Map",
    "center": [0, 0],
    "zoom": 2,
    "maxZoom": 18,
    "minZoom": 1,
    "type": "maplibre",
    "style": {
      "sources": {
        "osm-bg": {
          "type": "raster",
          "tiles": ["https://tile.openstreetmap.org/{z}/{x}/{y}.png"],
          "tileSize": 256,
          "attribution": "&copy; OpenStreetMap contributors"
        }
      },
      "layers": [
        {
          "id": "osm-background",
          "type": "raster",
          "source": "osm-bg"
        }
      ]
    }
  },
  "catalog": 
  {
    "label": "Demo Catalog",
    "tree": 
    [
      {
        "label": "Base Maps",
        "expanded": true,
        "children": [
          { "label": "OpenStreetMap (xyz)", "layerId": "osm", "checked": false },
          { "label": "Blue Marble (WMS)", "layerId": "bluemarble", "checked": false }
        ]
      },
      {
        "label": "Data Layers",
        "expanded": true,
        "children": [
          { "label": "Earthquakes", "layerId": "earthquake", "checked": false },
          { "label": "Transport Network", "layerId": "transport", "checked": true },
          {
            "label": "Environmental",
            "children": [
              { "label": "Air Quality", "layerId": "air", "checked": false },
              { "label": "Water Quality", "layerId": "water", "checked": false }
            ]
          }
        ]
      }
    ],
    "sources": [
      {
        "id": "xyz-source",
        "type": "raster",
        "service": "xyz",
        "url": "https://tile.openstreetmap.org/{z}/{x}/{y}.png",
        "tileSize": 256,
        "attribution": "Â© OpenStreetMap contributors"
      },
      {
        "id": "bluemarble-source",
        "type": "raster",
        "service": "wms",
        "url": "https://demo.mapserver.org/cgi-bin/wms?layers=bluemarble",
        "tileSize": 512,
        "attribution": "NASA Blue Marble"
      },
      {
        "id": "earthquake-source",
        "type": "geojson",
        "data": "https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary/all_day.geojson",
        "attribution": "USGS Earthquake Data"
      },
      {
        "id": "transport-source",
        "type": "geojson",
        "data": "https://data.example.com/transport_network.geojson"
      },
      {
        "id": "air-source",
        "type": "geojson",
        "data": "https://data.example.com/air_quality.geojson"
      },
      {
        "id": "water-source",
        "type": "geojson",
        "data": "https://data.example.com/water_quality.geojson"
      }
    ],
    "layers": [
      {
        "id": "osm",
        "layerset": [
          {
            "type": "raster",
            "source": "xyz-source"
          }
        ]
      },
      {
        "id": "bluemarble",
        "layerset": [
          {
            "type": "raster",
            "source": "bluemarble-source"
          }
        ]
      },
      {
        "id": "earthquake",
        "layerset": [
          {
            "type": "circle",
            "paint": {
              "circle-color": "#FF5722",
              "circle-radius": 14,
              "circle-opacity": 0.8,
              "circle-stroke-color": "#FFFFFF",
              "circle-stroke-width": 1
            },
            "source": "earthquake-source"
          }
        ]
      },
      {
        "id": "transport",
        "layerset": [
          {
            "type": "line",
            "source": "transport-source"
          }
        ]
      },
      {
        "id": "air",
        "layerset": [
          {
            "type": "circle",
            "source": "air-source"
          }
        ]
      },
      {
        "id": "water",
        "layerset": [
          {
            "type": "circle",
            "source": "water-source"
          }
        ]
      }
    ]
  },
  "tools": {
    "coordinates": { "enabled": true },
    "layerTree": { "enabled": true },
    "legend": { "enabled": true }
  }
}